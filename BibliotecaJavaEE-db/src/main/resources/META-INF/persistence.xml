<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="BibliotecaJavaEE_PU" transaction-type="JTA">
    <jta-data-source>java:app/jdbc/BIBLIOTECA_JEE</jta-data-source>
    <class>com.rxmxnx.bibliotecajavaee.db.entidad.AutorEntidad</class>
    <class>com.rxmxnx.bibliotecajavaee.db.entidad.GeneroEntidad</class>
    <class>com.rxmxnx.bibliotecajavaee.db.entidad.InventarioEntidad</class>
    <class>com.rxmxnx.bibliotecajavaee.db.entidad.LibroEntidad</class>
    <class>com.rxmxnx.bibliotecajavaee.db.entidad.PaisEntidad</class>
    <class>com.rxmxnx.bibliotecajavaee.db.entidad.PrestamoEntidad</class>
    <class>com.rxmxnx.bibliotecajavaee.db.entidad.UsuarioEntidad</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
        <property name="eclipselink.logging.level" value="FINE"/>
        <property name="eclipselink.logging.parameters" value="true"/>
        <property name="eclipselink.target-database" value="MySQL"/>
    </properties>
  </persistence-unit>
</persistence>
